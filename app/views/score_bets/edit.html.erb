<%= stylesheet_link_tag "score_bet" %>
<%= form_for(@score_bet, url: match_score_bets_path(match_id: params[:match_id])) do |f| %>
  <%= render "shared/error_messages", object: f.object %>

  <%= f.label :price %>
  <%= f.text_field :price, class: "form-control" %>

  <%= f.label :outcome %>
  <%= f.text_field :outcome, class: "form-control" %>

  <%= f.submit t("users.edit.save"), class: "btn btn-primary" %>
<% end %>
